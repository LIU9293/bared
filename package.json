{
  "name": "bared",
  "version": "0.0.24",
  "description": "Headless NodeJS CMS for better developer experience",
  "scripts": {
    "start": "node ./packages/demo/index.js",
    "format": "standard --fix",
    "dev": "lerna run dev --scope=@bared/demo",
    "build": "lerna run build",
    "patch": "lerna version --conventional-commits",
    "publish": "lerna publish --no-private"
  },
  "keywords": [
    "NodeJS",
    "Koa",
    "CMS"
  ],
  "author": "baredigit (hello@baredigit.com)",
  "license": "MIT",
  "dependencies": {
    "standard": "17.0.0"
  },
  "devDependencies": {
    "lerna": "^5.4.3"
  },
  "packages": [
    "packages/*"
  ],
  "workspaces": [
    "packages/*"
  ]
}
